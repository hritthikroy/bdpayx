{
  "version": 2,
  "builds": [
    {
      "src": "backend-go/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend-go/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/flutter_app/build/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
